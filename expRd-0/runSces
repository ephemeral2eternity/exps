# Creating folders
mkdir data
mkdir rsts

# Running Scripts of Sce0
mkdir rsts/Sce0/

# Running Sce0 for nonQoEAgent
java -jar nonQoEAgent.jar platformSce0.xml deployment.xml
mkdir rsts/Sce0/nonQoE
mv data/* rsts/Sce0/nonQoE/
# Running Sce0 for nonCoopAgent
java -jar nonCoopAgent.jar platformSce0.xml deployment.xml
mkdir rsts/Sce0/nonCoop
mv data/* rsts/Sce0/nonCoop/
# Running Sce0 for coopAgent
java -jar coopAgent.jar platformSce0.xml deployment.xml
mkdir rsts/Sce0/coop
mv data/* rsts/Sce0/coop/

