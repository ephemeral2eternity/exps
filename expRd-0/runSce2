# Creating folders
mkdir data
mkdir rsts

# Running Scripts of Sce3
mkdir rsts/Sce3/

# Running Sce3 for nonQoEAgent
java -jar nonQoEAgent.jar platformSce3.xml deployment.xml
mkdir rsts/Sce3/nonQoE
mv data/* rsts/Sce3/nonQoE/

# Running Sce3 for nonCoopAgent
java -jar nonCoopAgent.jar platformSce3.xml deployment.xml
mkdir rsts/Sce3/nonCoop
mv data/* rsts/Sce3/nonCoop/

# Running Sce3 for coopAgent
java -jar coopAgent.jar platformSce3.xml deployment.xml
mkdir rsts/Sce3/coop
mv data/* rsts/Sce3/coop/

