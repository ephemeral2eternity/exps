# Scenario 0
eval $COROUTINE_JAVA -jar nonQoEAgent.jar platformSce0.xml deployment.xml
mkdir rsts/nonQoESce0
mv data/* rsts/nonQoESce0/
eval $COROUTINE_JAVA -jar nonCoopAgent.jar platformSce0.xml deployment.xml
mkdir rsts/nonCoopSce0
mv data/* rsts/nonCoopSce0/
eval $COROUTINE_JAVA -jar coopAgent.jar platformSce0.xml deployment.xml
mkdir rsts/coopSce0
mv data/* rsts/coopSce0/
echo "Finishing Scenario 0"

